<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
   
<struts>
	<constant name="struts.devMode" value="true"/>
	<package name="ajp.struts" extends="struts-default">
		<action name="login" class="ajp.struts.Login">
			<result name="ok">home.jsp</result>
			<result name="notok">index.jsp</result>
		</action>		
	</package>
	<package name="ajp.strut" extends="struts-default">
		<action name="registration" class="ajp.strut.Register">
			<result name="ok">index.jsp</result>
			<result name="notok">registration.jsp</result>
		</action>
	</package>

</struts>