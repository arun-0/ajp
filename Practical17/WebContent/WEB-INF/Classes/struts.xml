<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
   "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
   "http://struts.apache.org/dtds/struts-2.0.dtd">
   
<struts>
	<package name="ajp.struts" extends="struts-default">
		<action name="registration" class="ajp.struts.Register">
			<result name="ok">login.jsp</result>
			<result name="notok">index.jsp</result>
		</action>
		<action name="login" class="ajp.struts.Login">
			<result name="ok">home.jsp</result>
			<result name="notok">login.jsp</result>
		</action>
	</package>
</struts>